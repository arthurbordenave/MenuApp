.from-inline.clearfix
    .nested-fields
        %table.table
            %tr
                %td= f.input :name, input_html: {class: 'form-input form-control'}
                %td= f.input :price, input_html: {class: 'form-input form-control'}
                %td= f.input :thc, input_html: {class: 'form-input form-control'}
                %td= f.input :cbd, input_html: {class: 'form-input form-control'}
                %td= f.input :genotype, input_html: {class: 'form-input form-control'}
        = link_to_remove_association 'Remove', f, class: 'form-button btn btn-default'
